          "mustPresent": [
            "WebView2_APICalled"
          ],
          "mustAbsent": [],
          "mayPresent": [
            "NavigationRequest::Create",
            "NavigationRequest::BeginNavigation"
          ],
          "timingKeys": [
            "navigation_total"
          ],
          "symptomKeywords": [
            "navigation",
            "completed",
            "not received",
            "stuck",
            "no callback"
